{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "applicationId": {
      "type": "integer",
      "format": "int32"
    },
    "name": {
      "type": "string",
      "maxLength": 10,
      "minLength": 0
    },
    "age": {
      "type": "string",
      "maximum": 30,
      "minimum": 18
    },
    "amount": {
      "type": "number",
      "format": "double",
      "maximum": 999.99,
      "minimum": 0
    }
  },
  "required": ["applicationId", "name", "age", "amount"]
}
